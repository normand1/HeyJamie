{
  "schemaVersion": 2,
  "transcriptId": "arxiv-quantum-rl-transformers",
  "description": "Validates BrowserOS navigates to arXiv cs.AI recent papers via direct command, then opens the Quantum Reinforcement Learning with Transformers paper (arXiv:2602.05920) via its HTML link instead of PDF",
  "expectations": [
    {
      "id": "navigate-to-arxiv-list-request",
      "event": "browseros-request",
      "match": {
        "triggerReason": "direct-command",
        "directCommandContains": "arxiv"
      }
    },
    {
      "id": "navigate-to-arxiv-list-response-ok",
      "event": "browseros-response",
      "match": {
        "browserosRunIndex": 0,
        "ok": true,
        "urlContains": "arxiv.org"
      }
    },
    {
      "id": "open-quantum-rl-paper-request",
      "event": "browseros-request",
      "match": {
        "triggerReason": "direct-command",
        "directCommandContains": "quantum"
      }
    },
    {
      "id": "open-quantum-rl-paper-response-ok",
      "event": "browseros-response",
      "match": {
        "browserosRunIndex": 1,
        "ok": true,
        "endUrlContains": "arxiv.org/html/2602.05920"
      }
    }
  ],
  "summary": {
    "runComplete": {
      "status": "success",
      "browserosRunsStarted": { "min": 2, "max": 2 },
      "browserosRunsSucceeded": { "min": 2, "max": 2 },
      "browserosRunsFailed": { "max": 0 }
    }
  }
}
